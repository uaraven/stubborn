<rules>
    <methods>
        <method>
            <returntype>java.lang.String</returntype>
            <methodname>get.*</methodname>
            <body>return "get-string";</body>
        </method>
        <method>
            <returntype>java.lang.String</returntype>
            <body>return "get-int";</body>
        </method>
    </methods>
</rules>
